#include<stdio.h>
#include<stdlib.h>
int main()
{
    int *p;
    p=(int*)malloc(sizeof(int)*10);
    int key,flag=0,i;
    printf("enter array elements\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&p[i]);
    }
    printf("enter key value\n");
    scanf("%d",&key);
    for(i=0;i<10;i++)
    {
        if((p[i])==key)
        {
            flag=1;
        }
    }
        if(flag==1)
        {
            printf("key found\n");
        }
        else
        {
            printf("key not found\n");
            return 0;
            
        }
}
